{
  "editor.tabSize": 2,
  "[latex]": {
    "editor.formatOnSave": false
  },
  "cSpell.language": "de-de",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    // Latex files
    "**/*.aux": true,
    "**/*.bbl": true,
    "**/*.blg": true,
    "**/*.bcf": true,
    "**/*.fdb_latexmk": true,
    "**/*.fls": true,
    "**/*.lof": true,
    "**/*.lol": true,
    "**/*.out": true,
    "**/*.run.xml": true,
    "**/*.synctex.gz": true,
    "**/*.toc": true,
    "**/*.lot": true
  }
}
